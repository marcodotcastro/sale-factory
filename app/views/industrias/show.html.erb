<div class="row">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel">
      <div class="x_title">
        <h2>Meu Perfil
          <small>...</small>
        </h2>
        <ul class="nav navbar-right panel_toolbox">
          <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
          </li>
        </ul>
        <div class="clearfix"></div>
      </div>
      <div class="nav navbar-right">
        <%#TODO: Mover para o helpers %>
        <% if current_usuario.industria? %>
          <%= link_to 'Editar Perfil', edit_industria_path(@industria), class: "btn btn-primary" %>
        <% end %>
      </div>
      <div class="x_content">
        <div class="divider-dashed"></div>
        <div class="col-sm-8 col-md-8 col-lg-8">
          <form data-parsley-validate class="form-horizontal form-label-left">
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Descrição </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <input type="text" readonly="readonly" class="form-control col-md-7 col-xs-12" value="<%= @industria.descricao %>">
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">CNPJ </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <input type="text" readonly="readonly" class="form-control col-md-7 col-xs-12" value="<%= @industria.cnpj %>">
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Setor </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <input type="text" readonly="readonly" class="form-control col-md-7 col-xs-12" , value="<%= @industria.setor.descricao %>">
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Cidade </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <input type="text" readonly="readonly" class="form-control col-md-7 col-xs-12" , value="<%= @industria.cidade.descricao %>">
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Endereço </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <input type="text" readonly="readonly" class="form-control col-md-7 col-xs-12" , value="<%= @industria.endereco %>">
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">CEP </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <input type="text" readonly="readonly" class="form-control col-md-7 col-xs-12" , value="<%= @industria.cep %>">
              </div>
            </div>

            <div class="divider-dashed"></div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Contato </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <input type="text" readonly="readonly" class="form-control col-md-7 col-xs-12" value="<%= @industria.contato %>">
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Telefone </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <input type="text" readonly="readonly" class="form-control col-md-7 col-xs-12" , value="<%= @industria.telefone %>">
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Whatsapp </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <input type="text" id="last-name" readonly="readonly" class="form-control col-md-7 col-xs-12" , value="<%= @industria.telefone_whatsapp %>">
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Email </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <input type="text" readonly="readonly" class="form-control col-md-7 col-xs-12" , value="<%= @industria.email %>">
              </div>
            </div>
          </form>
        </div>
        <div class="col-sm-4 col-md-4 col-lg-4">
          <%= image_tag foto_logo_url(@industria), class: "img-profile" %>
        </div>
      </div>
    </div>
  </div>
</div>

