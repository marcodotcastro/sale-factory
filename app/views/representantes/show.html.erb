<div class="row">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel">
      <div class="x_title">
        <h2>Meu Perfil
          <small>
            <b>Medalhas:</b>
            <% current_usuario.badges.each do |bagde| %>
              <%= bagde.name %> <%= (" e level " + bagde.level.to_s) if bagde.level %>,
            <% end %>
          </small>
        </h2>
        <ul class="nav navbar-right panel_toolbox">
          <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
          </li>
        </ul>
        <div class="clearfix"></div>
      </div>

      <%= render 'representante' %>
    </div>
  </div>
</div>

