<div class="row">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel">
      <div class="x_title">
        <h2>Cadastrar Lojista
          <small>...</small>
        </h2>
        <ul class="nav navbar-right panel_toolbox">
          <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
          </li>
        </ul>
        <div class="clearfix"></div>
      </div>
      <%= simple_form_for([@representante, @lojista], html: {class: "form-horizontal form-label-left"}) do |f| %>
        <div class="nav navbar-right">
          <%= f.button :submit, "Salvar Lojista", class: "btn btn-success" %>
        </div>
        <div class="x_content">
          <div class="divider-dashed"></div>
          <%= render 'form', f: f %>
        </div>
      <% end %>
    </div>
  </div>
</div>



