<%= simple_form_for([@cliente, @representante_comercial, @lojista]) do |f| %>

  <div class="form-inputs">
    <%= f.input :descricao %>
    <%= f.input :endereco %>
    <%= f.input :telefone %>
    <%= f.input :cep %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
