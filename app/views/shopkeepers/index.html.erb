<p id="notice"><%= notice %></p>

<h1>Shopkeepers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>State</th>
      <th>City</th>
      <th>Phone</th>
      <th>Cep</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shopkeepers.each do |shopkeeper| %>
      <tr>
        <td><%= shopkeeper.name %></td>
        <td><%= shopkeeper.address %></td>
        <td><%= shopkeeper.state %></td>
        <td><%= shopkeeper.city %></td>
        <td><%= shopkeeper.phone %></td>
        <td><%= shopkeeper.cep %></td>
        <td><%= shopkeeper.latitude %></td>
        <td><%= shopkeeper.longitude %></td>
        <td><%= link_to 'Show', shopkeeper %></td>
        <td><%= link_to 'Edit', edit_shopkeeper_path(shopkeeper) %></td>
        <td><%= link_to 'Destroy', shopkeeper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shopkeeper', new_shopkeeper_path %>
